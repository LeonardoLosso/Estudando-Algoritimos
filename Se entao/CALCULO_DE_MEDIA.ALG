algoritmo "semnome"
var
   M1, M2, MF: real
inicio
   escreva ("sua primeira nota: ")
   leia (m1)
   escreva ("sua segunda nota: ")
   leia (m2)
   MF<- (m1+m2)/2
        se MF>=6 entao
           escreva ("você está aprovado")
        senao
             escreva("você está reprovado")
        fimse

fimalgoritmo